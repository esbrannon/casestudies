{
  "name": "Menu Maker",
  "description": "A web app that generates recipes based on ingredients you have on hand.",
  "repository": "https://github.com/heroku-demos/menu-maker",
  "keywords": ["nodejs", "ai", "chatgpt", "openai", "react"],
  "environments": {
    "production": {
      "env": {
        "OPENAI_API_KEY": {
          "required": true
        }
      }
    },
    "test": {
      "env": {
        "OPENAI_API_KEY": "test_key"
      }
    }
  }
}
